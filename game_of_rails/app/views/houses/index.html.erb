<h2>Houses: <%= link_to "+", new_house_path %></h2>

<% @houses.each do |house| %>
  <p><%= link_to house.name, house_path(house) %>:
    <%= link_to "Edit", edit_house_path(house) %>
    <%= link_to "Delete", house_path(house), method: :delete%>
  </p>
  <img src="<%= house.img_url %>" alt="">

<% end %>
